<% if @users.any? %>
<ul class="list-unstyled">
  <% @users.each do |user| %>
  <li class="question">
    <%= link_to user.username, user_path(user) %>
    <% if current_user.admin %>
      <%= link_to image_tag("trashcan.png"), user_path(user), :class => 'delete',
      :data => {:confirm => "Really really?",
        :method => "delete"} %>
    <% end %>
    </li>
    <% end %>
  </ul>
  <% else %>
  <h5>No Users Yet</h5>
  <% end %>
